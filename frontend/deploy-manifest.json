{
  "version": 1,
  "routes": [
    {
      "path": "/_next/*",
      "target": { "kind": "Static", "cacheControl": "public, max-age=31536000, immutable" },
      "fallback": { "kind": "Compute", "src": "default" }
    },
    {
      "path": "/*.*",
      "target": { "kind": "Static" },
      "fallback": { "kind": "Compute", "src": "default" }
    },
    {
      "path": "/*",
      "target": { "kind": "Compute", "src": "default" }
    }
  ],
  "computeResources": [
    {
      "name": "default",
      "runtime": "nodejs20.x",
      "entrypoint": "server.js"
    }
  ],
  "framework": {
    "name": "nextjs",
    "version": "15.0.0"
  }
}
